    <!-- DataTables -->
    <link rel="stylesheet" href="~/WebCore/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/WebCore/plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
